          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_-b16EDivEe2P08UctWYbvg" name="userCheck" specification="_-byPsDivEe2P08UctWYbvg">
            <language>JAVA</language>
            <body>&#xD;
    int result = -1;&#xD;
    String sql = &quot;select pwd from members where id=?&quot;;&#xD;
    Connection conn = null;&#xD;
    PreparedStatement pstmt = null;&#xD;
    ResultSet rs = null;&#xD;
    try {&#xD;
        conn = getConnection();&#xD;
        pstmt = conn.prepareStatement(sql);&#xD;
        pstmt.setString(1, id);&#xD;
        rs = pstmt.executeQuery();&#xD;
        if (//해당 아이디가 존재하는 경우&#xD;
        rs.next()) {&#xD;
            if (rs.getString(&quot;pwd&quot;).equals(pwd) &amp;&amp; rs.getString(&quot;pwd&quot;) != null) {&#xD;
                //아이디가 존재하고 비밀번호가 일치하는 경우&#xD;
                result = 1;&#xD;
            } else //비밀번호가 없거나 일치하지 않는 경우&#xD;
            {&#xD;
                result = 0;&#xD;
            }&#xD;
        } else {&#xD;
            result = -1;&#xD;
        }&#xD;
    } catch (Exception e) {&#xD;
        System.out.println(&quot;로그인 중 오류 발생 : &quot; + e);&#xD;
    } finally {&#xD;
        try {&#xD;
            if (rs != null)&#xD;
                rs.close();&#xD;
            if (pstmt != null)&#xD;
                pstmt.close();&#xD;
            if (conn != null)&#xD;
                conn.close();&#xD;
        } catch (Exception ex) {&#xD;
            System.out.println(&quot;로그인 회선 종료 중 오류 발생 : &quot; + ex);&#xD;
        }&#xD;
    }&#xD;
    //계산된 결과를 return;&#xD;
    return result;&#xD;
</body>
